{
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "tabWidth": 2,
          "endOfLine": "auto"
        }
      ]
    }
  }